<resources>

    <declare-styleable name="CustomTabLayout">
        <attr name="tabIndicatorWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomAutoTextView">
        <attr name="autoWidthEnabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircularProgressView">
        <attr name="bgColor" format="reference|color" />
        <attr name="progressColor" format="reference|color" />
        <attr name="bgBarWidth" format="dimension" />
        <attr name="progressBarWidth" format="dimension" />
        <attr name="hideShadow" format="boolean" />
    </declare-styleable>

    <attr name="button_bg" format="reference|color" />
    <attr name="button_text" format="reference|color" />
    <attr name="page_bg" format="reference|color" />

</resources>
